{
  "General":{
    "lua_heading": "SERVER INFO",
    "parameters":{
      "Name":{
        "type": "string",
        "default": "New Server",
        "cmd": "ServerSettings.Description = $",
        "cmd_condition": ""
      },
      "Description":{
        "type": "string",
        "default": "About this server",
        "cmd": "ServerSettings.MOTD = $",
        "cmd_condition": ""
      },
      "Password":{
        "type": "string",
        "default": "",
        "cmd": "ServerSettings.Password = $",
        "cmd_condition": ""
      },
      "Notes":{
        "type": "multiple-string",
        "cmd": "-- $",
        "cmd_condition": ""
      }
    }
  },
  "Roles":{
    "lua_heading": "ADMINISTRATION",
    "parameters":{
      "Name":{
        "type": "string",
        "default": "New Server",
        "cmd": "",
        "cmd_condition": ""
